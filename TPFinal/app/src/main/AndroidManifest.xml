<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.neo.tpfinal">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:name=".db.FBO"
        android:allowBackup="true"
        android:icon="@drawable/saludable_ico"
        android:label="@string/app_name"
        android:roundIcon="@drawable/saludable_ico"
        android:supportsRtl="true"
        android:theme="@style/Theme.TPFinal">
        <activity android:name=".view.MainActivity"></activity>
        <activity android:name=".view.LoginActivity" />
        <activity android:name=".view.RegistrarActivity" />
        <activity
            android:name=".view.spash"
            android:label="@string/app_name"
            android:theme="@style/Theme.TPFinal.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>